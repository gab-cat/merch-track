{
    "builds": [{
        "src": "pip install -r requirements.txt",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "python merchtrack/manage.py runserver"
        }
    ]
}